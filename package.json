{ "name": "gulp-slm"
, "version": "1.0.0"
, "description": "Compile Slm to HTML for Gulp"
, "author": "Hyeon Kim <simnalamburt@gmail.com>"
, "homepage": "https://github.com/simnalamburt/gulp-slm"
, "bugs":
  { "url": "https://github.com/simnalamburt/gulp-slm/issues"
  }
, "license":
  { "type": "BSD 2-Clause"
  , "url": "https://github.com/simnalamburt/gulp-slm/blob/master/LICENSE.md"
  }
, "keywords":
